<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lz.manage.mapper.DormitoryApplyMapper">

    <resultMap type="DormitoryApply" id="DormitoryApplyResult">
        <result property="id"    column="id"    />
        <result property="buildingId"    column="building_id"    />
        <result property="dormitoryId"    column="dormitory_id"    />
        <result property="bedId"    column="bed_id"    />
        <result property="status"    column="status"    />
        <result property="applyContent"    column="apply_content"    />
        <result property="remark"    column="remark"    />
        <result property="userId"    column="user_id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectDormitoryApplyVo">
        select id, building_id, dormitory_id, bed_id, status, apply_content, remark, user_id, create_time, update_time from tb_dormitory_apply
    </sql>

    <select id="selectDormitoryApplyList" parameterType="DormitoryApply" resultMap="DormitoryApplyResult">
        <include refid="selectDormitoryApplyVo"/>
        <where>
            <if test="id != null ">and id = #{id}</if>
            <if test="buildingId != null ">and building_id = #{buildingId}</if>
            <if test="dormitoryId != null ">and dormitory_id = #{dormitoryId}</if>
            <if test="bedId != null ">and bed_id = #{bedId}</if>
            <if test="userId != null ">and user_id = #{userId}</if>
            <if test="status != null  and status != ''">and status = #{status}</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''">
                and DATE_FORMAT(create_time, '%Y-%m-%d') between #{params.beginCreateTime} and #{params.endCreateTime}
            </if>
        </where>
        order by create_time desc
    </select>

    <select id="selectDormitoryApplyById" parameterType="Long" resultMap="DormitoryApplyResult">
        <include refid="selectDormitoryApplyVo"/>
        where id = #{id}
    </select>

    <insert id="insertDormitoryApply" parameterType="DormitoryApply" useGeneratedKeys="true" keyProperty="id">
        insert into tb_dormitory_apply
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="buildingId != null">building_id,</if>
            <if test="dormitoryId != null">dormitory_id,</if>
            <if test="bedId != null">bed_id,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="applyContent != null and applyContent != ''">apply_content,</if>
            <if test="remark != null and remark != ''">remark,</if>
            <if test="userId != null">user_id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="buildingId != null">#{buildingId},</if>
            <if test="dormitoryId != null">#{dormitoryId},</if>
            <if test="bedId != null">#{bedId},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="applyContent != null and applyContent != ''">#{applyContent},</if>
            <if test="remark != null and remark != ''">#{remark},</if>
            <if test="userId != null">#{userId},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateDormitoryApply" parameterType="DormitoryApply">
        update tb_dormitory_apply
        <trim prefix="SET" suffixOverrides=",">
            <if test="buildingId != null">building_id = #{buildingId},</if>
            <if test="dormitoryId != null">dormitory_id = #{dormitoryId},</if>
            <if test="bedId != null">bed_id = #{bedId},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="applyContent != null and applyContent != ''">apply_content = #{applyContent},</if>
            <if test="remark != null and remark != ''">remark = #{remark},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDormitoryApplyById" parameterType="Long">
        delete from tb_dormitory_apply where id = #{id}
    </delete>

    <delete id="deleteDormitoryApplyByIds" parameterType="String">
        delete from tb_dormitory_apply where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
