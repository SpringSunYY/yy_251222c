<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lz.manage.mapper.DormitoryBedHistoryMapper">
    
    <resultMap type="DormitoryBedHistory" id="DormitoryBedHistoryResult">
        <result property="id"    column="id"    />
        <result property="buildingId"    column="building_id"    />
        <result property="dormitoryId"    column="dormitory_id"    />
        <result property="belongUserId"    column="belong_user_id"    />
        <result property="remark"    column="remark"    />
        <result property="userId"    column="user_id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectDormitoryBedHistoryVo">
        select id, building_id, dormitory_id, belong_user_id, remark, user_id, create_time, update_time from tb_dormitory_bed_history
    </sql>

    <select id="selectDormitoryBedHistoryList" parameterType="DormitoryBedHistory" resultMap="DormitoryBedHistoryResult">
        <include refid="selectDormitoryBedHistoryVo"/>
        <where>  
            <if test="id != null "> and id = #{id}</if>
            <if test="buildingId != null "> and building_id = #{buildingId}</if>
            <if test="dormitoryId != null "> and dormitory_id = #{dormitoryId}</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and DATE_FORMAT(create_time, '%Y-%m-%d') between #{params.beginCreateTime} and #{params.endCreateTime}</if>
        </where>
    </select>
    
    <select id="selectDormitoryBedHistoryById" parameterType="Long" resultMap="DormitoryBedHistoryResult">
        <include refid="selectDormitoryBedHistoryVo"/>
        where id = #{id}
    </select>

    <insert id="insertDormitoryBedHistory" parameterType="DormitoryBedHistory" useGeneratedKeys="true" keyProperty="id">
        insert into tb_dormitory_bed_history
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="buildingId != null">building_id,</if>
            <if test="dormitoryId != null">dormitory_id,</if>
            <if test="belongUserId != null">belong_user_id,</if>
            <if test="remark != null and remark != ''">remark,</if>
            <if test="userId != null">user_id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="buildingId != null">#{buildingId},</if>
            <if test="dormitoryId != null">#{dormitoryId},</if>
            <if test="belongUserId != null">#{belongUserId},</if>
            <if test="remark != null and remark != ''">#{remark},</if>
            <if test="userId != null">#{userId},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateDormitoryBedHistory" parameterType="DormitoryBedHistory">
        update tb_dormitory_bed_history
        <trim prefix="SET" suffixOverrides=",">
            <if test="buildingId != null">building_id = #{buildingId},</if>
            <if test="dormitoryId != null">dormitory_id = #{dormitoryId},</if>
            <if test="belongUserId != null">belong_user_id = #{belongUserId},</if>
            <if test="remark != null and remark != ''">remark = #{remark},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDormitoryBedHistoryById" parameterType="Long">
        delete from tb_dormitory_bed_history where id = #{id}
    </delete>

    <delete id="deleteDormitoryBedHistoryByIds" parameterType="String">
        delete from tb_dormitory_bed_history where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>